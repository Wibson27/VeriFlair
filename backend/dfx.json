{
  "version": 1,
  "canisters": {
    "auth": {
      "type": "rust",
      "candid": "src/auth/auth.did",
      "package": "auth"
    },
    "backend": {
      "type": "rust",
      "candid": "src/backend/backend.did",
      "package": "backend"
    },
    "nft": {
      "type": "rust",
      "candid": "src/nft/nft.did",
      "package": "nft"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://icp-api.io"],
      "type": "persistent"
    }
  },
  "output_env_file": ".env"
}